function [ ] = getPoseGraph( filesDir, windowLength )
%GETPOSEGRAPH Summary of this function goes here
%   Detailed explanation goes here
    
    fileExt = '*.txt';
    files = dir(fullfile(filesDir,fileExt)); 
    
    pathLength= length(files);
    
    for i=1:pathLength
        i
        
        fileCnt = num2str(i-1);
        fileName = [filesDir, fileCnt, '.txt'];     
        file_t = fopen(fileName);
        pointID = fscanf(file_t, '%d');
        
        for j=i+1:pathLength
            
            
            
        end
        
        
        pointID = fscanf(file_t, '%d');
        
    end
    

end

