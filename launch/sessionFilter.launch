<launch>

  <node pkg="learned_map" type="sessionFilter" name="sessionFilter" output="screen">

	<param name="loadMapName" value="/home/yh/mapModel/12.25/wholeMap_s10.vtk"/>
	<param name="saveMapName" value="/home/yh/mapModel/05.09/salient.vtk"/>
	<param name="staticInt" value="280"/> 

